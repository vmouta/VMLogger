<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>APPENDERS</key>
	<array>
		<dict>
			<key>headers</key>
			<dict>
				<key>apikey</key>
				<string>mykey</string>
			</dict>
			<key>method</key>
			<string>POST</string>
			<key>url</key>
			<string>https://testserver.something/log</string>
			<key>class</key>
			<string>VMLogger.URLLogAppend</string>
			<key>name</key>
			<string>server</string>
		</dict>
		<dict>
			<key>colors</key>
			<true/>
			<key>encoders</key>
			<dict>
				<key>pattern</key>
				<array>
					<string>%.30d [%thread][%Caller] %-7p %-20.-20c - %m</string>
				</array>
			</dict>
			<key>name</key>
			<string>console</string>
			<key>class</key>
			<string>VMLogger.ConsoleLogAppender</string>
		</dict>
		<dict>
			<key>name</key>
			<string>file</string>
			<key>class</key>
			<string>VMLogger.FileLogAppend</string>
			<key>fileName</key>
			<string>log.txt</string>
		</dict>
	</array>
	<key>LOGGER_CONFIG</key>
	<dict>
		<key>VMLogger_Example</key>
		<dict>
			<key>appenders</key>
			<array>
				<string>console</string>
			</array>
			<key>level</key>
			<string>Error</string>
			<key>additivity</key>
			<true/>
		</dict>
		<key>VMLogger_Example.AppDelegate</key>
		<dict>
			<key>level</key>
			<string>info</string>
			<key>additivity</key>
			<true/>
		</dict>
	</dict>
	<key>LOGGER_LEVEL</key>
	<string>verbose</string>
	<key>LOGGER_APPENDERS</key>
	<array>
		<string>console</string>
	</array>
</dict>
</plist>
